{
  "name": "tiatus",
  "version": "0.0.1",
  "description": "Tiatus is a unified timing system",
  "private": true,
  "scripts": {
    "build:aot:prod": "npm run clean && npm run ngc -p tsconfig.webpack.json -- --webpack webpack/webpack.prod.js && webpack --config webpack/webpack.prod.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "clean": "rimraf target/ts_compiled ${TARGET}/common ${TARGET}/vendor ${TARGET}/common ${TARGET}/polyfills ${TARGET}/adjudicator ${TARGET}/admin ${TARGET}/results ${TARGET}/timing ${TARGET}/assets",
    "build": "npm run build:aot",
    "ngc": "./node_modules/.bin/ngc -w -p tsconfig.webpack.json",
    "rimraf": "rimraf",
    "integration-test": "protractor webpack/protractor.conf.js",
    "postinstall": "npm run webdriver:update",
    "test": "karma start webpack/karma.conf.js",
    "tsc": "tsc",
    "lint": "npm run tslint ",
    "tslint": "tslint --type-check --project tsconfig.webpack.json --exclude node_modules",
    "start": "webpack-dev-server --config webpack/webpack.dev.js --progress --port 8082",
    "specs-build": "webpack --config webpack/webpack.test.js",
    "webdriver:update": "webdriver-manager update"
  },
