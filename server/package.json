{
  "name": "tiatus",
  "version": "0.0.1",
  "description": "Tiatus is a unified timing system",
  "private": true,
  "scripts": {
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && npm run ngc -- --webpack webpack/webpack.dev.js && webpack --config webpack/webpack.dev.js  --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "clean": "rimraf dist aot",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "build": "npm run clean && webpack --config webpack/webpack.dev.js --progress --profile && npm run test",
    "server:prod": "http-server dist -c-1 --cors",
    "ngc": "./node_modules/.bin/ngc-w -p tsconfig.webpack.json",
    "rimraf": "rimraf",
    "test": "karma start webpack/karma.conf.js",
    "start": "webpack-dev-server --config webpack/webpack.dev.js --inline --progress --port 8082"
  },
